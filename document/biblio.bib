@article{restrepoMuertesPorDesnutricion2020,
  title = {Muertes por desnutrici\'on en Am\'erica del Sur en los \'ultimos veinte a\~{n}os},
  author = {Restrepo, Gloria Deossa and Betancur, Luis Fernando Restrepo and Vargas, John Edison Vel\'asquez},
